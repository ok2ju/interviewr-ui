      a(href='#').c-header__name INTERVIEWR
      i.icon-down-open-big
    div.primary-nav-overlay
    nav.header-nav
      ul.header-nav__list
        li.header-nav__list-item
          a(href='#').header-nav__link CANDIDATES
        li.header-nav__list-item
          a(href='#').header-nav__link MESSAGES
        li.header-nav__list-item
          a(href='#').header-nav__link VACANCY
        li.header-nav__list-item
          a(href='#').header-nav__link SEARCH
    div.acc-photo
      include ico-bar.jade
      a(href='#').acc-photo-link
        i.icon-down-dir
        img(src='images/1.png').acc-photo__img
